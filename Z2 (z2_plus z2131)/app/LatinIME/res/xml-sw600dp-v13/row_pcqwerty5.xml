<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <Row latin:keyWidth="7.0000052%p">
        <include latin:keyWidth="9.000004%p" latin:keyboardLayout="@xml/key_settings" />
        <switch>
            <case latin:languageSwitchKeyEnabled="true">
                <Key latin:keyStyle="languageSwitchKeyStyle" latin:keyWidth="9.000004%p" latin:keyXPos="22.000004%p" />
                <Key latin:keyStyle="spaceKeyStyle" latin:keyWidth="39.999996%p" />
            </case>
            <default>
                <Key latin:keyStyle="spaceKeyStyle" latin:keyWidth="41.999996%p" latin:keyXPos="28.999996%p" />
            </default>
        </switch>
        <include latin:backgroundType="functional" latin:keyWidth="9.000004%p" latin:keyXPos="-9.000004%p" latin:keyboardLayout="@xml/key_emoji" />
    </Row>
</merge>