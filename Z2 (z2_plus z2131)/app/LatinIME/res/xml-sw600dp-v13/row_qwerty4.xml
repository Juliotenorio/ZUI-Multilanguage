<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:isSplitLayout="true">
            <Row latin:backgroundType="functional" latin:keyWidth="8.000004%p">
                <Key latin:keyStyle="toSymbolKeyStyle" />
                <include latin:keyboardLayout="@xml/key_comma" />
                <include latin:backgroundType="normal" latin:keyboardLayout="@xml/key_space_3kw" />
                <Spacer latin:keyWidth="20.000004%p" />
                <Key latin:keyStyle="spaceKeyStyle" latin:keyWidth="24.0%p" />
                <include latin:keyboardLayout="@xml/key_period" />
                <include latin:keyboardLayout="@xml/key_emoji" />
            </Row>
        </case>
        <default>
            <Row latin:backgroundType="functional" latin:keyWidth="9.000004%p">
                <Key latin:keyStyle="toSymbolKeyStyle" latin:keyWidth="10.000002%p" />
                <include latin:keyboardLayout="@xml/key_comma" />
                <include latin:backgroundType="normal" latin:keyXPos="19.000006%p" latin:keyboardLayout="@xml/key_space_7kw" />
                <include latin:keyboardLayout="@xml/key_period" />
                <include latin:keyboardLayout="@xml/key_emoji" />
            </Row>
        </default>
    </switch>
</merge>