<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSet="hindi_compact">
            <key-style latin:moreKeys="◌ॉ|ॉ" latin:styleName="moreKeysDevanagariVowelSignCandraO" />
        </case>
        <case latin:keyboardLayoutSet="marathi">
            <key-style latin:moreKeys="ऑ" latin:styleName="moreKeysDevanagariVowelSignCandraO" />
        </case>
        <default>
            <key-style latin:styleName="moreKeysDevanagariVowelSignCandraO" />
        </default>
    </switch>
    <key-style latin:keySpec="◌ॉ|ॉ" latin:keyLabelFlags="followKeyLetterRatio" latin:styleName="baseKeyDevanagariVowelSignCandraO" latin:parentStyle="moreKeysDevanagariVowelSignCandraO" />
</merge>