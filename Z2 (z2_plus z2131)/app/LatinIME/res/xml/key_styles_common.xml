<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLockShifted">
            <key-style latin:keyLabelFlags="hasShiftedLetterHint|shiftedLetterActivated" latin:styleName="hasShiftedLetterHintStyle" />
        </case>
        <default>
            <key-style latin:keyLabelFlags="hasShiftedLetterHint" latin:styleName="hasShiftedLetterHintStyle" />
        </default>
    </switch>
    <include latin:keyboardLayout="@xml/key_styles_settings" />
    <key-style latin:moreKeys="!noPanelAutoMoreKey!, |!code/key_capslock" latin:keyActionFlags="noKeyPreview" latin:keyLabelFlags="preserveCase" latin:styleName="baseForShiftKeyStyle" />
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetAutomaticShifted">
            <key-style latin:backgroundType="stickyOff" latin:keySpec="!icon/shift_key_shifted|!code/key_shift" latin:styleName="shiftKeyStyle" latin:parentStyle="baseForShiftKeyStyle" />
        </case>
        <case latin:keyboardLayoutSetElement="alphabetShiftLocked|alphabetShiftLockShifted">
            <key-style latin:backgroundType="stickyOn" latin:keySpec="!icon/shift_key_shifted|!code/key_shift" latin:styleName="shiftKeyStyle" latin:parentStyle="baseForShiftKeyStyle" />
        </case>
        <default>
            <key-style latin:backgroundType="stickyOff" latin:keySpec="!icon/shift_key|!code/key_shift" latin:styleName="shiftKeyStyle" latin:parentStyle="baseForShiftKeyStyle" />
        </default>
    </switch>
    <key-style latin:backgroundType="functional" latin:keyActionFlags="isRepeatable|noKeyPreview" latin:keySpec="!icon/delete_key|!code/key_delete" latin:styleName="deleteKeyStyle" />
    <switch>
        <case latin:keyboardTheme="ICS|KLP">
            <key-style latin:backgroundType="action" latin:keyActionFlags="noKeyPreview" latin:keySpec="!icon/emoji_action_key|!code/key_emoji" latin:styleName="emojiKeyStyle" />
        </case>
        <default>
            <key-style latin:backgroundType="action" latin:keyActionFlags="noKeyPreview" latin:keySpec="!icon/emoji_action_key|!code/key_emoji" latin:keyLabelFlags="keepBackgroundAspectRatio" latin:styleName="emojiKeyStyle" />
        </default>
    </switch>
    <include latin:keyboardLayout="@xml/key_styles_enter" />
    <key-style latin:backgroundType="spacebar" latin:keyActionFlags="enableLongPress|noKeyPreview" latin:keySpec="!icon/space_key|!code/key_space" latin:styleName="spaceKeyStyle" />
    <key-style latin:moreKeys="!icon/zwj_key|‍" latin:keyActionFlags="noKeyPreview" latin:keySpec="!icon/zwnj_key|‌" latin:keyLabelFlags="hasPopupHint" latin:styleName="zwnjKeyStyle" />
    <key-style latin:altCode="!code/key_space" latin:keyActionFlags="altCodeWhileTyping|noKeyPreview" latin:keySpec="!icon/shortcut_key|!code/key_shortcut" latin:keyIconDisabled="!icon/shortcut_key_disabled" latin:styleName="shortcutKeyStyle" latin:parentStyle="settingsMoreKeysStyle" />
    <key-style latin:altCode="!code/key_space" latin:backgroundType="functional" latin:keyActionFlags="altCodeWhileTyping|noKeyPreview" latin:keySpec="!icon/settings_key|!code/key_settings" latin:styleName="settingsKeyStyle" />
    <key-style latin:altCode="!code/key_space" latin:keyActionFlags="altCodeWhileTyping|enableLongPress|noKeyPreview" latin:keySpec="!icon/language_switch_key|!code/key_language_switch" latin:styleName="languageSwitchKeyStyle" />
    <key-style latin:backgroundType="functional" latin:keySpec="!icon/tab_key|!code/key_tab" latin:styleName="tabKeyStyle" />
    <key-style latin:keySpec="!icon/tab_key|!code/key_tab" latin:styleName="nonSpecialBackgroundTabKeyStyle" />
    <key-style latin:backgroundType="functional" latin:keyActionFlags="noKeyPreview" latin:keyLabelFlags="followFunctionalTextColor|preserveCase" latin:styleName="baseForLayoutSwitchKeyStyle" />
    <key-style latin:keySpec="!text/keylabel_to_symbol|!code/key_switch_alpha_symbol" latin:styleName="toSymbolKeyStyle" latin:parentStyle="baseForLayoutSwitchKeyStyle" />
    <key-style latin:keySpec="!text/keylabel_to_alpha|!code/key_switch_alpha_symbol" latin:styleName="toAlphaKeyStyle" latin:parentStyle="baseForLayoutSwitchKeyStyle" />
    <key-style latin:keySpec="!text/keylabel_to_more_symbol|!code/key_shift" latin:styleName="toMoreSymbolKeyStyle" latin:parentStyle="baseForLayoutSwitchKeyStyle" />
    <key-style latin:keySpec="!text/keylabel_to_symbol|!code/key_shift" latin:styleName="backFromMoreSymbolKeyStyle" latin:parentStyle="baseForLayoutSwitchKeyStyle" />
    <key-style latin:moreKeys="!text/morekeys_popular_domain" latin:backgroundType="functional" latin:keySpec="!text/keyspec_popular_domain" latin:keyLabelFlags="autoXScale|fontNormal|hasPopupHint|preserveCase" latin:styleName="comKeyStyle" />
</merge>