<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:clobberSettingsKey="true">
            <key-style latin:backgroundType="functional" latin:styleName="settingsMoreKeysStyle" />
        </case>
        <default>
            <key-style latin:additionalMoreKeys="!text/keyspec_settings" latin:backgroundType="functional" latin:keyLabelFlags="hasPopupHint" latin:styleName="settingsMoreKeysStyle" />
        </default>
    </switch>
</merge>