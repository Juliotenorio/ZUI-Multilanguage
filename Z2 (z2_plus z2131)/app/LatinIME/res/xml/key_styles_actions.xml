<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:isIconDefined="go_key">
            <key-style latin:keySpec="!icon/go_key|!code/key_enter" latin:styleName="goActionKeyStyle" latin:parentStyle="defaultEnterKeyStyle" />
        </case>
        <default>
            <key-style latin:keySpec="!text/label_go_key|!code/key_enter" latin:styleName="goActionKeyStyle" latin:parentStyle="defaultEnterKeyStyle" />
        </default>
    </switch>
    <switch>
        <case latin:isIconDefined="next_key">
            <key-style latin:keySpec="!icon/next_key|!code/key_enter" latin:styleName="nextActionKeyStyle" latin:parentStyle="defaultEnterKeyStyle" />
        </case>
        <default>
            <key-style latin:keySpec="!text/label_next_key|!code/key_enter" latin:styleName="nextActionKeyStyle" latin:parentStyle="defaultEnterKeyStyle" />
        </default>
    </switch>
    <switch>
        <case latin:isIconDefined="previous_key">
            <key-style latin:keySpec="!icon/previous_key|!code/key_enter" latin:styleName="previousActionKeyStyle" latin:parentStyle="defaultEnterKeyStyle" />
        </case>
        <default>
            <key-style latin:keySpec="!text/label_previous_key|!code/key_enter" latin:styleName="previousActionKeyStyle" latin:parentStyle="defaultEnterKeyStyle" />
        </default>
    </switch>
    <switch>
        <case latin:isIconDefined="done_key">
            <key-style latin:keySpec="!icon/done_key|!code/key_enter" latin:styleName="doneActionKeyStyle" latin:parentStyle="defaultEnterKeyStyle" />
        </case>
        <default>
            <key-style latin:keySpec="!text/label_done_key|!code/key_enter" latin:styleName="doneActionKeyStyle" latin:parentStyle="defaultEnterKeyStyle" />
        </default>
    </switch>
    <switch>
        <case latin:isIconDefined="send_key">
            <key-style latin:keySpec="!icon/send_key|!code/key_enter" latin:styleName="sendActionKeyStyle" latin:parentStyle="defaultEnterKeyStyle" />
        </case>
        <default>
            <key-style latin:keySpec="!text/label_send_key|!code/key_enter" latin:styleName="sendActionKeyStyle" latin:parentStyle="defaultEnterKeyStyle" />
        </default>
    </switch>
    <switch>
        <case latin:isIconDefined="search_key">
            <key-style latin:keySpec="!icon/search_key|!code/key_enter" latin:styleName="searchActionKeyStyle" latin:parentStyle="defaultEnterKeyStyle" />
        </case>
        <default>
            <key-style latin:keySpec="!text/label_search_key|!code/key_enter" latin:styleName="searchActionKeyStyle" latin:parentStyle="defaultEnterKeyStyle" />
        </default>
    </switch>
    <switch>
        <case latin:keyboardTheme="ICS|KLP">
            <key-style latin:backgroundType="action" latin:keySpec="dummy_label|!code/key_enter" latin:keyLabelFlags="fromCustomActionLabel" latin:styleName="customLabelActionKeyStyle" latin:parentStyle="defaultEnterKeyStyle" />
        </case>
        <default>
            <key-style latin:backgroundType="functional" latin:keySpec="dummy_label|!code/key_enter" latin:keyLabelFlags="fromCustomActionLabel" latin:styleName="customLabelActionKeyStyle" latin:parentStyle="defaultEnterKeyStyle" />
        </default>
    </switch>
</merge>