<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <Key latin:additionalMoreKeys="1" latin:keySpec="q" latin:keyHintLabel="1" />
    <Key latin:moreKeys="!text/morekeys_w" latin:additionalMoreKeys="2" latin:keySpec="w" latin:keyHintLabel="2" />
    <Key latin:additionalMoreKeys="3" latin:keySpec="f" latin:keyHintLabel="3" />
    <Key latin:additionalMoreKeys="4" latin:keySpec="p" latin:keyHintLabel="4" />
    <Key latin:moreKeys="!text/morekeys_g" latin:additionalMoreKeys="5" latin:keySpec="g" latin:keyHintLabel="5" />
    <Key latin:moreKeys="!text/morekeys_j" latin:additionalMoreKeys="6" latin:keySpec="j" latin:keyHintLabel="6" />
    <Key latin:moreKeys="!text/morekeys_l" latin:additionalMoreKeys="7" latin:keySpec="l" latin:keyHintLabel="7" />
    <Key latin:moreKeys="!text/morekeys_u" latin:additionalMoreKeys="8" latin:keySpec="u" latin:keyHintLabel="8" />
    <Key latin:moreKeys="!text/morekeys_y" latin:additionalMoreKeys="9" latin:keySpec="y" latin:keyHintLabel="9" />
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLockShifted">
            <Key latin:additionalMoreKeys="0" latin:keySpec=":" latin:keyHintLabel="0" />
        </case>
        <default>
            <Key latin:moreKeys=":" latin:additionalMoreKeys="0" latin:keySpec=";" latin:keyHintLabel="0" />
        </default>
    </switch>
</merge>