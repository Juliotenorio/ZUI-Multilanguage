<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:mode="email|url">
            <Key latin:keyStyle="comKeyStyle" latin:keyWidth="fillRight" />
        </case>
        <default>
            <Key latin:keyStyle="emojiKeyStyle" latin:keyWidth="fillRight" />
        </default>
    </switch>
</merge>