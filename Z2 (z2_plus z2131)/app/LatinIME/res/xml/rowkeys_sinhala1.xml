<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <Key latin:keySpec="ූ" />
            <Key latin:keySpec="උ" />
            <Key latin:keySpec="ෑ" />
            <Key latin:keySpec="ඍ" />
            <Key latin:keySpec="ඔ" />
            <Key latin:keySpec="ශ" />
            <Key latin:keySpec="ඹ" />
            <Key latin:keySpec="ෂ" />
            <Key latin:keySpec="ධ" />
            <Key latin:keySpec="ඡ" />
            <Key latin:moreKeys="෴" latin:keySpec="ඥ" />
        </case>
        <default>
            <Key latin:additionalMoreKeys="1" latin:keySpec="ු" latin:keyHintLabel="1" />
            <Key latin:additionalMoreKeys="2" latin:keySpec="අ" latin:keyHintLabel="2" />
            <Key latin:additionalMoreKeys="3" latin:keySpec="ැ" latin:keyHintLabel="3" />
            <Key latin:additionalMoreKeys="4" latin:keySpec="ර" latin:keyHintLabel="4" />
            <Key latin:additionalMoreKeys="5" latin:keySpec="එ" latin:keyHintLabel="5" />
            <Key latin:additionalMoreKeys="6" latin:keySpec="හ" latin:keyHintLabel="6" />
            <Key latin:additionalMoreKeys="7" latin:keySpec="ම" latin:keyHintLabel="7" />
            <Key latin:additionalMoreKeys="8" latin:keySpec="ස" latin:keyHintLabel="8" />
            <Key latin:moreKeys="ඳ" latin:additionalMoreKeys="9" latin:keySpec="ද" latin:keyHintLabel="9" />
            <Key latin:additionalMoreKeys="0" latin:keySpec="ච" latin:keyHintLabel="0" />
            <Key latin:moreKeys="෴" latin:keySpec="ඤ" />
        </default>
    </switch>
</merge>