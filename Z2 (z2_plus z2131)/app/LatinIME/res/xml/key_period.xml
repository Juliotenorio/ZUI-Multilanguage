<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSet="dvorak">
            <Key latin:moreKeys="!text/morekeys_punctuation,!text/morekeys_z" latin:keySpec="z" latin:keyLabelFlags="hasPopupHint" />
        </case>
        <default>
            <Key latin:moreKeys="!text/morekeys_period" latin:backgroundType="functional" latin:keySpec="!text/keyspec_period" latin:keyHintLabel="!text/keyhintlabel_period" latin:keyLabelFlags="hasPopupHint|hasShiftedLetterHint" />
        </default>
    </switch>
</merge>