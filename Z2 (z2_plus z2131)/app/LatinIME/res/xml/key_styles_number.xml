<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <key-style latin:keyActionFlags="noKeyPreview" latin:styleName="numKeyBaseStyle" />
    <key-style latin:keyLabelFlags="followKeyLargeLetterRatio" latin:styleName="numKeyStyle" latin:parentStyle="numKeyBaseStyle" />
    <key-style latin:keyLabelFlags="followFunctionalTextColor|followKeyLetterRatio|fontNormal" latin:styleName="numModeKeyStyle" latin:parentStyle="numKeyBaseStyle" />
    <key-style latin:styleName="numSymbolKeyStyle" latin:parentStyle="numKeyBaseStyle" />
    <key-style latin:keyLabelFlags="alignLabelOffCenter|hasHintLabel" latin:styleName="numberKeyStyle" latin:parentStyle="numKeyStyle" />
    <key-style latin:keySpec="0" latin:styleName="num0KeyStyle" latin:parentStyle="numberKeyStyle" />
    <key-style latin:keySpec="1" latin:styleName="num1KeyStyle" latin:parentStyle="numberKeyStyle" />
    <key-style latin:keySpec="2" latin:keyHintLabel="ABC" latin:styleName="num2KeyStyle" latin:parentStyle="numberKeyStyle" />
    <key-style latin:keySpec="3" latin:keyHintLabel="DEF" latin:styleName="num3KeyStyle" latin:parentStyle="numberKeyStyle" />
    <key-style latin:keySpec="4" latin:keyHintLabel="GHI" latin:styleName="num4KeyStyle" latin:parentStyle="numberKeyStyle" />
    <key-style latin:keySpec="5" latin:keyHintLabel="JKL" latin:styleName="num5KeyStyle" latin:parentStyle="numberKeyStyle" />
    <key-style latin:keySpec="6" latin:keyHintLabel="MNO" latin:styleName="num6KeyStyle" latin:parentStyle="numberKeyStyle" />
    <key-style latin:keySpec="7" latin:keyHintLabel="PQRS" latin:styleName="num7KeyStyle" latin:parentStyle="numberKeyStyle" />
    <key-style latin:keySpec="8" latin:keyHintLabel="TUV" latin:styleName="num8KeyStyle" latin:parentStyle="numberKeyStyle" />
    <key-style latin:keySpec="9" latin:keyHintLabel="WXYZ" latin:styleName="num9KeyStyle" latin:parentStyle="numberKeyStyle" />
    <key-style latin:keySpec="!text/keylabel_to_phone_symbols|!code/key_switch_alpha_symbol" latin:styleName="numPhoneToSymbolKeyStyle" latin:parentStyle="numModeKeyStyle" />
    <key-style latin:keySpec="!text/keylabel_to_phone_numeric|!code/key_switch_alpha_symbol" latin:styleName="numPhoneToNumericKeyStyle" latin:parentStyle="numModeKeyStyle" />
    <key-style latin:keyActionFlags="noKeyPreview" latin:styleName="numTabKeyStyle" latin:parentStyle="tabKeyStyle" />
    <key-style latin:keyActionFlags="enableLongPress" latin:keySpec="!icon/space_key_for_number_layout|!code/key_space" latin:keyLabelFlags="alignIconToBottom" latin:styleName="numSpaceKeyStyle" latin:parentStyle="numKeyBaseStyle" />
    <switch>
        <case latin:keyboardTheme="ICS|KLP">
            <key-style latin:backgroundType="functional" latin:keyActionFlags="enableLongPress" latin:keySpec="!icon/space_key|!code/key_space" latin:styleName="tabletNumSpaceKeyStyle" latin:parentStyle="numKeyBaseStyle" />
        </case>
        <case latin:keyboardTheme="LXXLight|LXXDark">
            <key-style latin:backgroundType="spacebar" latin:keyActionFlags="enableLongPress" latin:keySpec="!icon/space_key|!code/key_space" latin:styleName="tabletNumSpaceKeyStyle" latin:parentStyle="numKeyBaseStyle" />
        </case>
    </switch>
</merge>