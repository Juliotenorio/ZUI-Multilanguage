<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <Key latin:moreKeys="!text/morekeys_star" latin:keySpec="*" />
    <switch>
        <case latin:languageCode="fa">
            <Key latin:moreKeys="!text/morekeys_double_quote" latin:keySpec="!text/keyspec_left_double_angle_quote" />
            <Key latin:moreKeys="!text/morekeys_single_quote" latin:keySpec="!text/keyspec_right_double_angle_quote" />
        </case>
        <default>
            <Key latin:moreKeys="!text/morekeys_double_quote" latin:keySpec="&quot;" />
            <Key latin:moreKeys="!text/morekeys_single_quote" latin:keySpec="'" />
        </default>
    </switch>
    <Key latin:keySpec=":" />
    <Key latin:moreKeys="!text/morekeys_symbols_semicolon" latin:keySpec="!text/keyspec_symbols_semicolon" />
    <Key latin:moreKeys="!text/morekeys_exclamation" latin:keySpec="!" />
    <Key latin:moreKeys="!text/morekeys_question" latin:keySpec="!text/keyspec_symbols_question" />
</merge>