<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabet|alphabetAutomaticShifted|alphabetShiftLocked">
            <Key latin:additionalMoreKeys="~" latin:keySpec="`" />
            <Key latin:moreKeys="!text/morekeys_symbols_1" latin:additionalMoreKeys="!,!text/morekeys_exclamation" latin:keySpec="1" />
            <Key latin:moreKeys="!text/morekeys_symbols_2" latin:additionalMoreKeys="\@" latin:keySpec="2" />
            <Key latin:moreKeys="!text/morekeys_symbols_3" latin:additionalMoreKeys="\#" latin:keySpec="3" />
            <Key latin:moreKeys="!text/morekeys_symbols_4" latin:additionalMoreKeys="$" latin:keySpec="4" />
            <Key latin:moreKeys="!text/morekeys_symbols_5" latin:additionalMoreKeys="\\%" latin:keySpec="5" />
            <Key latin:moreKeys="!text/morekeys_symbols_6" latin:additionalMoreKeys="^" latin:keySpec="6" />
            <Key latin:moreKeys="!text/morekeys_symbols_7" latin:additionalMoreKeys="&amp;" latin:keySpec="7" />
            <Key latin:moreKeys="!text/morekeys_symbols_8" latin:additionalMoreKeys="*" latin:keySpec="8" />
            <Key latin:moreKeys="!text/morekeys_symbols_9" latin:additionalMoreKeys="(" latin:keySpec="9" />
            <Key latin:moreKeys="!text/morekeys_symbols_0" latin:additionalMoreKeys=")" latin:keySpec="0" />
            <Key latin:moreKeys="–,—,·" latin:additionalMoreKeys="_" latin:keySpec="-" />
            <Key latin:moreKeys="!fixedColumnOrder!4,∞,≠,≈,%" latin:additionalMoreKeys="+" latin:keySpec="=" />
        </case>
        <default>
            <include latin:keyboardLayout="@xml/rowkeys_pcqwerty1_shift" />
        </default>
    </switch>
</merge>