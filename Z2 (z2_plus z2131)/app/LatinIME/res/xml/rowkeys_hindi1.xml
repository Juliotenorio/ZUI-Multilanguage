<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <Key latin:moreKeys="ऒं" latin:keySpec="औ" />
            <Key latin:moreKeys="ऐं" latin:keySpec="ऐ" />
            <Key latin:moreKeys="आं,आँ" latin:keySpec="आ" />
            <Key latin:moreKeys="ईं" latin:keySpec="ई" />
            <Key latin:moreKeys="ऊं,ऊँ" latin:keySpec="ऊ" />
            <Key latin:keySpec="भ" />
            <include latin:keyboardLayout="@xml/keystyle_devanagari_sign_visarga" />
            <Key latin:keyStyle="baseKeyDevanagariSignVisarga" />
            <Key latin:keySpec="घ" />
            <Key latin:moreKeys="क्ष,श्र" latin:keySpec="ध" />
            <Key latin:keySpec="झ" />
            <Key latin:keySpec="ढ" />
        </case>
        <default>
            <include latin:keyboardLayout="@xml/keystyle_devanagari_vowel_sign_au" />
            <Key latin:additionalMoreKeys="१,1" latin:keyHintLabel="1" latin:keyStyle="baseKeyDevanagariVowelSignAu" />
            <include latin:keyboardLayout="@xml/keystyle_devanagari_vowel_sign_ai" />
            <Key latin:additionalMoreKeys="२,2" latin:keyHintLabel="2" latin:keyStyle="baseKeyDevanagariVowelSignAi" />
            <include latin:keyboardLayout="@xml/keystyle_devanagari_vowel_sign_aa" />
            <Key latin:additionalMoreKeys="३,3" latin:keyHintLabel="3" latin:keyStyle="baseKeyDevanagariVowelSignAa" />
            <include latin:keyboardLayout="@xml/keystyle_devanagari_vowel_sign_ii" />
            <Key latin:additionalMoreKeys="४,4" latin:keyHintLabel="4" latin:keyStyle="baseKeyDevanagariVowelSignIi" />
            <include latin:keyboardLayout="@xml/keystyle_devanagari_vowel_sign_uu" />
            <Key latin:additionalMoreKeys="५,5" latin:keyHintLabel="5" latin:keyStyle="baseKeyDevanagariVowelSignUu" />
            <Key latin:moreKeys="ब॒,%" latin:additionalMoreKeys="६,6" latin:keySpec="ब" latin:keyHintLabel="6" />
            <Key latin:additionalMoreKeys="७,7" latin:keySpec="ह" latin:keyHintLabel="7" />
            <Key latin:moreKeys="ज्ञ,ग़,ग॒,%" latin:additionalMoreKeys="८,8" latin:keySpec="ग" latin:keyHintLabel="8" />
            <Key latin:additionalMoreKeys="९,9" latin:keySpec="द" latin:keyHintLabel="9" />
            <Key latin:moreKeys="ज॒,ज्ञ,ज़,%" latin:additionalMoreKeys="०,0" latin:keySpec="ज" latin:keyHintLabel="0" />
            <Key latin:moreKeys="ड॒,ड़" latin:keySpec="ड" />
        </default>
    </switch>
</merge>