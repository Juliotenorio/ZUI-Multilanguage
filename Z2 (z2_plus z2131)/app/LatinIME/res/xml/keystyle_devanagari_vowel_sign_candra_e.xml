<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSet="hindi_compact">
            <key-style latin:moreKeys="◌ॅ|ॅ" latin:styleName="moreKeysDevanagariVowelSignCandraE" />
        </case>
        <case latin:keyboardLayoutSet="marathi">
            <key-style latin:moreKeys="ऍ" latin:styleName="moreKeysDevanagariVowelSignCandraE" />
        </case>
        <default>
            <key-style latin:styleName="moreKeysDevanagariVowelSignCandraE" />
        </default>
    </switch>
    <key-style latin:keySpec="◌ॅ|ॅ" latin:keyLabelFlags="followKeyLetterRatio" latin:styleName="baseKeyDevanagariVowelSignCandraE" latin:parentStyle="moreKeysDevanagariVowelSignCandraE" />
</merge>